{
  "name": "sensylate",
  "version": "1.0.0",
  "description": "Sensylate: Sensitivity analysis meets portfolio simulation and strategy creation.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:pwa": "tsc && vite build --mode production",
    "preview": "vite preview",
    "serve:pwa": "vite preview --port 5000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test:e2e": "npx puppeteer browsers install && node tests/testRunner.js",
    "test:e2e-workflow": "node tests/endToEndWorkflow.spec.js",
    "test:e2e-screenshots": "node tests/endToEndWorkflow.spec.js --screenshots",
    "test:e2e-verbose": "node tests/endToEndWorkflow.spec.js --verbose --screenshots",
    "test:screenshots": "node tests/testRunner.js --screenshots",
    "test:workflow": "node tests/parameterTesting.spec.js",
    "test:ci": "node tests/testRunner.js --ci --save-report",
    "test:verbose": "node tests/testRunner.js --verbose --screenshots",
    "test:cleanup-screenshots": "node tests/screenshotCleanup.js",
    "test:cleanup-screenshots-verbose": "node tests/screenshotCleanup.js --verbose",
    "test:advanced-config": "node tests/test-advanced-config.js",
    "test:position-sizing": "node tests/runPositionSizingTest.js",
    "test:position-sizing-screenshots": "node tests/runPositionSizingTest.js --screenshots",
    "test:position-sizing-verbose": "node tests/runPositionSizingTest.js --verbose --screenshots --headed",
    "test:position-sizing-full": "node tests/runPositionSizingTest.js --screenshots --full",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@tanstack/react-table": "^8.10.7",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "graphql": "^16.11.0",
    "papaparse": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-pwa": "^1.0.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "puppeteer": "^24.9.0",
    "typescript": "^5.0.2",
    "vite": "^6.2.6"
  }
}
